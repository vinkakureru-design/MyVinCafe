            padding: 8px 20px;
            border: 1px solid #eee;
        }

        .status-item .label {
            color: #888;
            margin-right: 5px;
        }

        .status-item .value {
            font-weight: 700;
            letter-spacing: 1px;
        }

        .navbar-area {
            background: #fdfdfd;
        }

        .icon-btn {
            display: flex;
            align-items: center;
            justify-content: center;
            width: 40px;
            height: 40px;
            background: #f8f9fa;
            border: 1px solid #ddd;
        }

            .icon-btn:hover {
                background: var(--german-dark);
                color: white !important;
            }

